#include <QApplication>
#include <QTextCodec>
#include "game.h"

static Game* game;

int main(int argc, char *argv[]){
    QApplication a(argc, argv);

    QTextCodec *codec = QTextCodec::codecForName("UTF-8");
    QTextCodec::setCodecForLocale(codec);

    game = new Game();

    return a.exec();
}
